{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/messages/:userId",
        "dest": "/index.js" 
      },
      {
        "src": "/people",
        "dest": "/index.js" 
      },
      {
        "src": "/profile",
        "dest": "/index.js" 
      },
      {
        "src": "/login",
        "dest": "/index.js" 
      },
      {
        "src": "/logout",
        "dest": "/index.js" 
      },
      {
        "src": "/register",
        "dest": "/index.js" 
      }
    ]
  }
  